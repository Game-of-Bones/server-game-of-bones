{
  "name": "server-game-of-bones",
  "version": "1.0.0",
  "description": "API REST para Game of Bones - Blog de PaleontologÃ­a",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "dev:test": "cross-env NODE_ENV=test nodemon --exec ts-node src/server.ts --silent --verbose",
    "start": "node dist/server.js",
    "build": "tsc",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "seed:dev": "cross-env NODE_ENV=development ts-node src/seeders/dev/runDevSeeders.ts",
    "seed:test": "cross-env NODE_ENV=test ts-node src/seeders/test/runTestSeeders.ts",
    "init:test": "cross-env NODE_ENV=test ts-node src/database/initTest.ts",
    "db:fresh": "ts-node -e \"require('./src/models').syncDatabase(true).then(() => process.exit(0))\"",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Game-of-Bones/server-game-of-bones.git"
  },
  "keywords": [
    "paleontologia",
    "api",
    "rest",
    "nodejs",
    "typescript",
    "sequelize"
  ],
  "author": "Game of Bones Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Game-of-Bones/server-game-of-bones/issues"
  },
  "homepage": "https://github.com/Game-of-Bones/server-game-of-bones#readme",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "mysql2": "^3.15.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.3",
    "@types/validator": "^13.15.3",
    "cross-env": "^10.1.0",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
